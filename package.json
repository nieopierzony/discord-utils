{
  "name": "discord-utils",
  "version": "0.1.0",
  "description": "Utilities for discord-interactions",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && npm run types && metatests test/",
    "types": "tsc -p tsconfig.json",
    "lint": "eslint . && prettier --check \"**/*.js\" \"**/*.json\" \"**/*.md\" \"**/.*rc\" \"**/*.ts\"",
    "fmt": "prettier --write \"**/*.js\" \"**/*.json\" \"**/*.md\" \"**/.*rc\" \"**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nieopierzony/discord-utils.git"
  },
  "keywords": [
    "discord",
    "utils",
    "embed"
  ],
  "author": "Philipp Zelinski <filipbon7@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nieopierzony/discord-utils/issues"
  },
  "homepage": "https://github.com/nieopierzony/discord-utils#readme",
  "devDependencies": {
    "@types/node": "^17.0.18",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "metatests": "^0.8.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  }
}
